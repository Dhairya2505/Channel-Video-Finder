{
    "name": "Channel Video Searcher",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "index.html"
    },
    "permissions": [
        "activeTab"
    ],
    "content_scripts":[
        {
            "matches": ["https://www.youtube.com/*"],
            "js": ["content.js"]
        }
    ]
  }